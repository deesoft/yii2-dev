{
    "name": "deesoft/yii2-dev",
    "description": "Yii2 libraries development project",
    "keywords": ["yii2", "extension", "main"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Misbahul Munir",
            "email": "misbahuldmunir@gmail.com",
            "role": "Core project development"
        }
    ],
    "support": {
        "issues": "https://github.com/deesoft/yii2-dev/issues",
        "source": "https://github.com/deesoft/yii2-dev"
    },
    "minimum-stability": "dev",
    "replace": {
        "deesoft/yii2-gii": "*",
        "deesoft/yii2-console": "*",
        "deesoft/yii2-angular": "*",
        "deesoft/yii2-rest": "*",
        "deesoft/yii2-base": "*",
        "deesoft/yii2-adminlte": "*",
        "deesoft/biz3-api": "*",
        "deesoft/biz3-client": "*",
        "mdmsoft/yii2-admin": "~3.0"
    },
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.4",
        "yiisoft/yii2-bootstrap": "~2.0",
        "yiisoft/yii2-authclient": "~2.0.0",
        "mdmsoft/yii2-autonumber": "~1.0",
        "mdmsoft/yii2-format-converter": "~1.0",
        "mdmsoft/yii2-ar-behaviors": "~1.0",
        "deesoft/yii2-composer": "~1.0",
        "bower-asset/AdminLTE": "~2.0",
        "rmrevin/yii2-fontawesome": "~2.0",
        "bower-asset/angular": "~1.3",
        "bower-asset/angular-route": "~1.3",
        "bower-asset/angular-resource": "~1.3",
        "bower-asset/angular-animate": "~1.3",
        "bower-asset/angular-bootstrap": "~0.13",
        "bower-asset/angular-validation": "~1.3"
    },
    "require-dev": {
        "deesoft/yii2-gii": "~1.0",
        "yiisoft/yii2-gii": "~2.0",
        "yiisoft/yii2-debug": "~2.0"
    },
    "suggest": {
        "codeception/codeception": "Codeception, 1.8.*@dev is currently works well with Yii.",
        "codeception/specify": "BDD style code blocks for PHPUnit and Codeception",
        "codeception/verify": "BDD Assertions for PHPUnit and Codeception"
    },
    "config": {
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "dee\\gii\\": "libraries/gii",
            "dee\\console\\": "libraries/console",
            "dee\\base\\": "libraries/base",
            "dee\\rest\\": "libraries/rest",
            "dee\\angular\\": "libraries/angular",
            "dee\\adminlte\\": "libraries/adminlte",
            "mdm\\admin\\": "libraries/admin",
            "biz\\api\\": "libraries/biz-api",
            "biz\\client\\": "libraries/biz-client",
            "dee\\": ""
        }
    },
    "extra": {
        "bootstrap": "dee\\Bootstrap",
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "symlinks": {
            "application/vendor": "vendor",
            "init": "application/init",
            "yii": "application/yii"
        }
    }
}
